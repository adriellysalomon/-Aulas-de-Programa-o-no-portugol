programa {
	funcao inicio() {
        inteiro tab
        inteiro numero
        
        escreva("Digite um numero para visualizar sua respectiva tabuada")
        leia(numero)
  
        para (inteiro c=1; c<=10; c++)  
        {  
            tab=c*numero
            escreva (numero, " x ", c, " = ", tab, "\n")  
        }  
    }  
}  
	

